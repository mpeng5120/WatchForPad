<?xml version="1.0" encoding="UTF-8"?>
<excel>
	<!-- 
		必填项为:name,header，其他全部选填
		
		title:excel表格中第一行合并单元格居中显示的内容
		description：第二行显示的说明
		author：xml作者，不会输出
		sheet:导出excel的sheet名
		columns:导出列的信息
			column:一个列
				name:列对应Object中的字段名，必须有标准的get方法
				header:导出excel中显示的标题
				width:列宽(单位像素)
	 -->
	<title>监视信息</title>
	<description>TRA铁人机器人智能工厂监控系统   正常监视信息</description>
	<author>isea533</author>
	<sheet>监视信息</sheet>
	<columns>
		<column>
			<name>devNumber</name>
			<header>显示编号</header>
			<width>80</width>
		</column>
		<column>
			<name>deviceName</name>
			<header>机器编号</header>
			<width>100</width>
		</column>
		<column>
			<name>IpAdd</name>
			<header>Wifi名称</header>
			<width>150</width>
		</column>
		<column>
			<name>Info</name>
			<header>其他信息</header>
                        <width>150</width>
		</column>
		<column>
			<name>RunningStatus</name>
			<header>运行状态</header>
                        <width>150</width>
		</column>
		<column>
			<name>TargetNum</name>
			<header>目标生产数量</header>
			<width>100</width>
		</column>
                <column>
			<name>CompletedNum</name>
			<header>生产数量</header>
			<width>100</width>
		</column>
                <column>
			<name>RemainNum</name>
			<header>剩余数量</header>
			<width>100</width>
		</column>
                <column>
			<name>ConnectionStatus</name>
			<header>连接状态</header>
			<width>100</width>
		</column>
	</columns>
</excel>